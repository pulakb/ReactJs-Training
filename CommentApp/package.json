{
  "author": "Jack Callister",
  "name": "flux-comment",
  "version": "0.0.1",
  "description": "Experimenting with the flux pattern",
  "repository": "https://github.com/jarsbe/flux-comment",
  "dependencies": {
    "flux": "*",
    "object-assign": "*",
    "react": "*",
    "node-http-server": "*"
  },
  "devDependencies": {
    "jsx-loader": "*",
    "reactify": "*",
